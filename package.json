{
  "name": "Nevermore",
  "main": "index.html",
  "description": "shadowsocks client powered by Nodejs",
  "version": "0.0.1",
  "keywords": [
    "shadowsocks",
    "node-webkit"
  ],
  "single-instance": true,
  "dependencies": {
    "shadowsocks": "*"
  },
  "window": {
    "icon": "img/icon.png",
    "frame": false,
    "toolbar": false,
    "width": 300,
    "height": 400,
    "position": "mouse",
    "min_width": 300,
    "min_height": 400,
    "max_width": 500,
    "max_height": 400
  },
  "js-flags": "--expose-gc",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "devDependencies": {
    "node-webkit-builder": "^0.1.3"
  }
}
